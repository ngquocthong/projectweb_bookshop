{{ form_start(form) }}
<div class="row">
	<div class="col-md-6 col-lg-4">
		<div class="form-group has-success">
			{{ form_row(form.name, {'attr': {'class': 'form-control'}}) }}
			<div class="form-group">
				{{ form_row(form.publisher, {'attr': {'class': 'form-control'}}) }}
			</div>
			<div class="form-group">
				{{ form_row(form.category, {'attr': {'class': 'form-control'}}) }}
			</div>
			<div class="form-group has-success">
				{{ form_row(form.description, {'attr': {'class': 'form-control'}}) }}
			</div>
			<div class="form-group has-success">
				{{ form_row(form.price, {'attr': {'class': 'form-control'}}) }}
			</div>
			<div class="form-group">
				{{ form_row(form.publishDate, {'attr': {'class': 'form-control'}}) }}
			</div>
			<div class="form-group">
				{{ form_row(form.image, {'attr': {'class': 'form-control'}}) }}
			</div>
			<div class="form-group">
				{{ form_row(form.printlength, {'attr': {'class': 'form-control'}}) }}
			</div>


		</div>
	</div>
</div>
<div class="row mt-3">
	<div class="col-md-6 col-lg-4">
		<div class="form-group">
			<button class="btn btn-success">{{ button_label|default('Save') }}</button>
		</div>
	</div>
</div>
{{ form_end(form) }}
